@{
    ViewData["Title"] = "Vehiculo";
}

<div class="container mt-4">
    <div class="row">

        <div class="col-8">
            <h2 class="text-dark">Lista de Vehiculos</h2>
        </div>

        <div class="col-4 text-right">
            <a asp-action="Create" class="btn btn-primary bg-dark">Crear Nuevo Vehiculo</a>
            <i class="fa-solid fa-plus"></i>
        </div>

    </div>

    <br />
    <br />

    <table id="tblVehiculo" class=" table table-striped table-bordered  ">
        <thead class="thead-dark">
            <tr>
                <th>Id</th>
                <th>Marca</th>
                <th>Modelo</th>
                <th>Tipo</th>
                <th>Año</th>
                <th>Color</th>

                <th>Fecha de Creacion</th>
                <th>Fecha de Actualizacion</th>
                <th>Estado</th>
                <th>Acciones</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/Vehiculos.js"></script>
}