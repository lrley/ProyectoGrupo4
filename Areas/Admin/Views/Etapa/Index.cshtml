@{
    ViewData["Title"] = "Etapa";
}

<div class="container mt-4">
    <div class="row">

        <div class="col-8">
            <h2 class="text-dark">Lista de Etapas</h2>
        </div>

        <div class="col-4 text-right">
            <a asp-action="Create" class="btn btn-primary bg-dark">Crear Nueva Etapa</a>
            <i class="fa-solid fa-plus"></i>
        </div>

    </div>

    <br />
    <br />

    <table id="tblEtapa" class=" table table-striped table-bordered ">
        <thead class="thead-dark">
            <tr>
                <th>Id</th>
                <th>Nombre de Etapa</th>
                <th>Nomobre de Urbanizacion</th>
               

                <th>Fecha de Creacion</th>
                <th>Fecha de Actualizacion</th>
                <th>Estado</th>
                <th>Acciones</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/Etapa.js"></script>
}